# Environment variables for Create React App
# Variables must be prefixed with REACT_APP_ to be accessible in the browser

REACT_APP_MOCK_API=true
REACT_APP_API_DELAY_MS=1000
REACT_APP_NY_TIMES_API_KEY=your_api_key_here
REACT_APP_API_BASE_URL=https://api.nytimes.com/svc

# Proxy configuration
REACT_APP_PROXY_ENABLED=true
REACT_APP_CORS_PROXY_URL=https://cors-anywhere.herokuapp.com
REACT_APP_USE_LOCAL_PROXY=true

# Development settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info
